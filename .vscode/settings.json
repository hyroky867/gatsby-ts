{
  "eslint.nodePath": "./node_modules/eslint",
  "eslint.packageManager": "yarn",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "files.exclude": {
    "node_modules/": true,
    "**/.history": true
  },
  "search.exclude": {
    "yarn.lock": true
  }
}
